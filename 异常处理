import java.util.*;
/*
异常处理

*/

public class exception {

	public static void main(String[] args) {
		Scanner i=new Scanner(System.in);
		int a=9;
		try {
			int b=i.nextInt();
			System.out.println(a/b);
		}catch(InputMismatchException e) {
			System.out.println("大兄弟，你输入的不对呀！");
		}finally {
			System.out.println("*****");
		}
		
		System.out.println("bye!");
	}

}
